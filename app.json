{
	"expo": {
		"name": "Tap Me",
		"slug": "tapme",
		"scheme": "tapme",
		"version": "1.0.3",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"userInterfaceStyle": "automatic",
			"bundleIdentifier": "com.tap.tapio",
			"associatedDomains": ["applinks:www.tapme.contact"]
		},
		"android": {
			"useNextNotificationsApi": true,
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.tap.tapio",
			"userInterfaceStyle": "automatic",
			"versionCode": 1,
			"intentFilters": [
				{
					"action": "VIEW",
					"autoVerify": true,
					"data": [
						{
							"scheme": "https",
							"host": "www.tapme.contact",
							"pathPrefix": "/contact"
						},
						{
							"scheme": "https",
							"host": "www.tapme.contact",
							"pathPrefix": "/transaction"
						}
					],
					"category": ["BROWSABLE", "DEFAULT"]
				}
			]
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"extra": {
			"eas": {
				"projectId": "59bf2110-fc1b-4882-9f6d-00049308c311"
			}
		}
	}
}
